<?xml version="1.0" encoding="UTF-8"?>
<hazelcast
        xsi:schemaLocation="http://www.hazelcast.com/schema/config http://www.hazelcast.com/schema/config/hazelcast-config-3.5.xsd"
        xmlns="http://www.hazelcast.com/schema/config"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <network>
        <join>
            <multicast enabled="true"/>
        </join>
    </network>
    <management-center enabled="true" update-interval="2">
        http://localhost:8080/mancenter
    </management-center>

    <cache name="capitals">
        <key-type class-name="java.lang.String"/>
        <value-type class-name="java.lang.String"/>
        <statistics-enabled>true</statistics-enabled>
    </cache>

    <cache name="test">
        <key-type class-name="java.lang.Integer"/>
        <value-type class-name="java.lang.String"/>
        <statistics-enabled>true</statistics-enabled>
    </cache>
</hazelcast>
