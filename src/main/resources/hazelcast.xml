<?xml version="1.0" encoding="UTF-8"?>
<hazelcast
        xsi:schemaLocation="http://www.hazelcast.com/schema/config http://www.hazelcast.com/schema/config/hazelcast-config-3.6.xsd"
        xmlns="http://www.hazelcast.com/schema/config"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <group>
        <name>jcache</name>
        <password>jcache</password>
    </group>

    <network>
        <join>
            <multicast enabled="true"/>
        </join>
    </network>
    <management-center enabled="true" update-interval="2">
        http://localhost:8080/mancenter
    </management-center>

    <cache name="capitals">
        <key-type class-name="java.lang.String"/>
        <value-type class-name="java.lang.String"/>
        <statistics-enabled>true</statistics-enabled>
    </cache>

    <cache name="test">
        <key-type class-name="java.lang.Integer"/>
        <value-type class-name="java.lang.String"/>
        <statistics-enabled>true</statistics-enabled>
    </cache>

    <cache name="city">
        <statistics-enabled>true</statistics-enabled>
    </cache>

    <cache name="default">
        <statistics-enabled>true</statistics-enabled>
    </cache>

    <quorum enabled="true" name="cache-quorum">
        <quorum-size>2</quorum-size>
        <quorum-type>READ_WRITE</quorum-type>
    </quorum>

    <!-- quorum example -->
    <cache name="cache-with-quorum">
        <statistics-enabled>true</statistics-enabled>
        <quorum-ref>cache-quorum</quorum-ref>
    </cache>
</hazelcast>
